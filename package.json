{
  "name": "WRTE",
  "version": "0.0.1",
  "description": "Web RunTime Environment",
  "main": "kernel/main.js",
  "repository": "https://github.com/smokku/wrte.git",
  "author": "Tomasz Sterna <smoku@xiaoka.com>",
  "license": "AGPL-3.0",
  "devDependencies": {
    "@babel/core": "7.0.0",
    "@babel/plugin-syntax-object-rest-spread": "7.0.0",
    "@babel/preset-flow": "7.0.0",
    "babel-eslint": "9.0.0",
    "babel-plugin-discard-module-references": "1.1.2",
    "babel-plugin-istanbul": "5.0.1",
    "documentation": "documentationjs/documentation#fb6ebdb",
    "eslint": "5.5.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-standard": "12.0.0",
    "eslint-config-standard-jsdoc": "0.3.2",
    "eslint-plugin-flowtype": "2.50.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsdoc": "3.8.0",
    "eslint-plugin-node": "7.0.1",
    "eslint-plugin-promise": "4.0.0",
    "eslint-plugin-standard": "4.0.0",
    "eslint-plugin-tape": "1.1.0",
    "eslint-plugin-unicorn": "6.0.1",
    "flow-bin": "0.80.0",
    "git-rev-sync": "1.12.0",
    "live-server": "1.2.0",
    "npm-run-all": "4.1.3",
    "nyc": "13.0.1",
    "puppeteer-core": "1.7.0",
    "rollup": "0.65.0",
    "rollup-plugin-babel": "4.0.2",
    "rollup-plugin-replace": "2.0.0",
    "rollup-plugin-root-import": "0.2.3",
    "tap-junit": "2.0.0",
    "tap-spec": "5.0.0"
  },
  "dependencies": {
    "tape-rollup": "4.6.4"
  },
  "scripts": {
    "build": "rm -rf dist/current && rollup -c",
    "watch": "rollup -c -w",
    "serve": "live-server --verbose --no-browser --cors dist/",
    "start": "npm-run-all --parallel watch serve",
    "lint": "eslint '{kernel,lib,cmd,test}/**.js'",
    "flow": "flow check",
    "pretest": "npm-run-all --parallel lint flow && NODE_ENV=test npm run build",
    "test": "node test/runner.js | tee test.log | grep -v '^:cov ' | tap-spec",
    "predocs": "cd node_modules/documentation && npm install",
    "docs": "documentation build 'kernel/**.js' 'lib/**.js' 'cmd/**.js' -f html -o docs"
  }
}

{
  "name": "WRTE",
  "version": "0.0.1",
  "description": "Web RunTime Environment",
  "main": "kernel/main.js",
  "repository": "https://github.com/smokku/wrte.git",
  "author": "Tomasz Sterna <smoku@xiaoka.com>",
  "license": "AGPL-3.0",
  "devDependencies": {
    "@babel/core": "7.0.0-beta.53",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0-beta.53",
    "@babel/plugin-syntax-object-rest-spread": "7.0.0-beta.53",
    "@babel/preset-env": "7.0.0-beta.53",
    "@babel/preset-flow": "7.0.0-beta.53",
    "@babel/register": "7.0.0-beta.53",
    "babel-eslint": "8.2.5",
    "babel-plugin-discard-module-references": "1.1.2",
    "babel-plugin-istanbul": "4.1.6",
    "browser-run": "5.0.0",
    "documentation": "github:documentationjs/documentation#fb6ebdb",
    "eslint": "5.1.0",
    "eslint-config-airbnb-base": "13.0.0",
    "eslint-config-standard": "11.0.0",
    "eslint-config-standard-jsdoc": "0.3.2",
    "eslint-plugin-flowtype": "2.50.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jsdoc": "3.7.1",
    "eslint-plugin-node": "6.0.1",
    "eslint-plugin-promise": "3.8.0",
    "eslint-plugin-standard": "3.1.0",
    "eslint-plugin-tape": "1.1.0",
    "eslint-plugin-unicorn": "4.0.3",
    "flow-bin": "0.76.0",
    "git-rev-sync": "1.12.0",
    "live-server": "1.2.0",
    "npm-run-all": "4.1.3",
    "nyc": "12.0.2",
    "rollup": "0.62.0",
    "rollup-plugin-babel": "4.0.0-beta.7",
    "rollup-plugin-replace": "2.0.0",
    "rollup-plugin-root-import": "0.2.3",
    "tap-junit": "2.0.0",
    "tap-spec": "5.0.0",
    "tape-rollup": "4.6.4"
  },
  "scripts": {
    "build": "rm -rf dist/current && rollup -c",
    "watch": "rollup -c -w",
    "dev": "npm-run-all --parallel start watch",
    "start": "live-server --verbose --no-browser --cors dist/",
    "pretest": "NODE_ENV=test npm run build",
    "test": "cat dist/current/*.js dist/current/*/*.js | browser-run --static ./dist/ | tee test.log | grep -v '^:cov ' | npx tap-spec",
    "predocs": "cd node_modules/documentation && npm install",
    "docs": "documentation build 'kernel/**.js' 'lib/**.js' 'cmd/**.js' -f html -o docs"
  }
}
